<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mmednet.common">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".multithread.MultiThreadActivity" />
        <activity android:name=".multithread.ImageTest" />
        <activity android:name=".multithread.ProgressBarTest" />
        <activity android:name=".multithread.ThreadTest" />
        <activity android:name=".multithread.HandlerThreadActivity" />
        <activity android:name=".download.DownloadActivity" />

        <service
            android:name=".download.DownloadService"
            android:enabled="true"
            android:exported="true" />

        <meta-data
            android:name="XMHT_APPNAME"
            android:value="test" />

        <activity android:name=".ipc.IPCActivity" />

        <service
            android:name=".ipc.IPCService"
            android:enabled="true"
            android:exported="true"
            android:process="com.qq.send.service"></service>
    </application>

</manifest>
